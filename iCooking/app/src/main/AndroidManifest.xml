<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.win81pro.icooking2" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/imgcooklogo"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:theme="@style/my_style" >
        <activity
            android:name=".Credit2"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Profile"
            android:label="@string/title_activity_profile" >
        </activity>
        <activity
            android:name=".MyActivity"
            android:label="@string/title_activity_credit" >
        </activity>
        <activity
            android:name=".Setting"
            android:label="@string/title_activity_setting" >
        </activity>
        <activity
            android:name=".Bookmark"
            android:label="@string/title_activity_bookmark" >
        </activity>
        <activity
            android:name=".PreBookmark"
            android:label="@string/title_activity_pre_bookmark" >
        </activity>
        <activity
            android:name=".Chat"
            android:label="@string/title_activity_chat" >
        </activity>
        <activity
            android:name=".ChatBubbleActivity"
            android:label="@string/title_activity_chat_bubble" >
        </activity>

        <!-- <activity -->
        <!-- android:name=".ChatArrayAdapter" -->
        <!-- android:label="@string/title_activity_chat_array_adapter" > -->
        <!-- </activity> -->
        <activity
            android:name=".ChatMessage"
            android:label="@string/title_activity_chat_message" >
        </activity>
        <activity
            android:name=".Cook"
            android:label="@string/title_activity_cook" >
            <intent-filter>
            <action android:name="android.intent.action.VIEW" />
            <category android:name="android.intent.category.DEFAULT" />
            <data android:scheme="Html" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Material"
            android:label="@string/title_activity_material" >
        </activity>
        <activity
            android:name=".Howto"
            android:label="@string/title_activity_howto" >
        </activity>
        <activity
            android:name=".Vedio"
            android:label="@string/title_activity_vedio" >
        </activity>
    </application>

</manifest>
